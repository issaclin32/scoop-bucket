    "extract_dir": "msdos",
    "pre_install": [
        "if ($architecture -eq '64bit') { Move-Item \"$dir\\binary\\*_x64\" \"$dir\\\" }",
        "elseif ($architecture -eq '32bit') { Move-Item \"$dir\\binary\\*_x86\" \"$dir\\\" }",
        "Remove-Item \"$dir\\binary\", \"$dir\\source\" -Force -Recurse"
    ],
    "architecture": {
        "64bit": {
            "bin": [
                [
                    "v30_x64\\msdos.exe",
                    "msdos-v30"
                ],
                [
                    "ia32_x64\\msdos.exe",
                    "msdos-ia32"
                ],
                [
                    "i86_x64\\msdos.exe",
                    "msdos-i8086"
                ],
                [
                    "i286_x64\\msdos.exe",
                    "msdos-i80286"
                ],
                [
                    "i386_x64\\msdos.exe",
                    "msdos-i386"
                ],
                [
                    "i486_x64\\msdos.exe",
                    "msdos-i486"
                ]
            ]
        },
        "32bit": {
            "bin": [
                [
                    "v30_x86\\msdos.exe",
                    "msdos-v30"
                ],
                [
                    "ia32_x86\\msdos.exe",
                    "msdos-ia32"
                ],
                [
                    "i86_x86\\msdos.exe",
                    "msdos-i8086"
                ],
                [
                    "i286_x86\\msdos.exe",
                    "msdos-i80286"
                ],
                [
                    "i386_x86\\msdos.exe",
                    "msdos-i386"
                ],
                [
                    "i486_x86\\msdos.exe",
                    "msdos-i486"
                ]
            ]
        }
    },
    "checkver": {
        "url": "http://takeda-toshiya.my.coocan.jp/msdos/history/index.html",
